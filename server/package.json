{
  "name": "fitness-tracker-server",
  "version": "1.0.0",
  "description": "Backend for fitness tracking app with JoJo AI",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:deploy": "npx prisma migrate deploy",
    "db:generate": "npx prisma generate",
    "db:seed": "node src/utils/seed.js",
    "db:studio": "npx prisma studio",
    "postinstall": "npx prisma generate"
  },
  "keywords": [
    "fitness",
    "tracking",
    "express",
    "ai",
    "groq"
  ],
  "author": "Adigun Anjolaoluwa",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "prisma": "^6.9.0"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "sharp": "^0.34.2",
    "winston": "^3.17.0"
  }
}